        "                    dz = df(z)\n",
        "                    if dz == 0:\n",
        "                        break\n",
        "\n",
        "                    count += 1\n",
        "                    if count > 255:\n",
        "                        break\n",
        "                   \n",
        "                    znext = z - f(z) / dz # Newton iteration\n",
