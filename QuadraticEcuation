        "x = np.linspace(-2.0,2.0,100)\n",
        "fig, ax = plt.subplots()\n",
        "ax.plot(x,f(x))\n",
        "ax.axhline(y=0, color='k')\n",
        "ax.axvline(x=0, color='k')\n",
        "ax.grid(True, which='both')\n",
        "plt.title('f(x)=x^2-1')\n",
        "plt.xlabel('x')\n",
        "plt.ylabel('f(x)')\n",
        "\n",
        "\n"
      ],
